{
    "name": "ghjayce/shipshape",
    "description": "Shipshape will keep your business code clean and tidy.",
    "keywords": ["shipshape", "clean", "tidy", "business"],
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Ghjayce\\Shipshape\\": "src/"
        }
    },
    "bin": [
        "bin/shipshape"
    ],
    "require": {
        "php": "^8.0.2",
        "psr/container": "^2.0"
    },
    "authors": [
        {
            "name": "GHJayce",
            "email": "jaycedeng@outlook.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^10.5|^9.6",
        "symfony/console": "^6.0.19",
        "symfony/finder": "^6.0.19"
    },
    "autoload-dev": {
        "psr-4": {
            "GhjayceTest\\Shipshape\\": "tests/",
            "GhjayceExample\\Shipshape\\": "example/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    }
}
